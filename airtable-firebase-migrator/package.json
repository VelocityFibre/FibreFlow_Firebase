{
  "name": "airtable-firebase-migrator",
  "version": "1.0.0",
  "description": "Migration tool for transferring data from Airtable to Firebase",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "migrate": "ts-node src/cli.ts migrate",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["airtable", "firebase", "migration", "etl"],
  "author": "FibreFlow Team",
  "license": "MIT",
  "dependencies": {
    "airtable": "^0.12.2",
    "commander": "^11.0.0",
    "dotenv": "^16.3.1",
    "firebase-admin": "^12.0.0",
    "ora": "^6.3.1",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
