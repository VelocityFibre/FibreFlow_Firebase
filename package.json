{
  "name": "fibreflow",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write \"src/**/*.{ts,html,scss,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,scss,json}\"",
    "check": "npm run lint && npm run format:check && npm run build",
    "check:fix": "npm run lint:fix && npm run format",
    "predeploy": "npm run check",
    "deploy": "firebase deploy",
    "deploy:hosting": "firebase deploy --only hosting",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:rules": "firebase deploy --only firestore:rules",
    "build:prod": "ng build --configuration production",
    "deploy:main": "./deploy-worktree.sh main",
    "deploy:boq": "./deploy-worktree.sh boq",
    "deploy:rfq": "./deploy-worktree.sh rfq",
    "deploy:tasks": "./deploy-worktree.sh tasks",
    "deploy:reports": "./deploy-worktree.sh reports",
    "deploy:hotfix": "./deploy-worktree.sh hotfix",
    "deploy:perf": "./deploy-worktree.sh perf",
    "worktree:status": "git worktree list"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.6",
    "@angular/cdk": "^20.0.5",
    "@angular/common": "^20.0.6",
    "@angular/compiler": "^20.0.6",
    "@angular/core": "^20.0.6",
    "@angular/fire": "^20.0.1",
    "@angular/forms": "^20.0.6",
    "@angular/material": "^20.0.5",
    "@angular/platform-browser": "^20.0.6",
    "@angular/platform-browser-dynamic": "^20.0.6",
    "@angular/router": "^20.0.6",
    "@emailjs/browser": "^4.4.1",
    "@sentry/angular": "^9.30.0",
    "@types/jspdf": "^1.3.3",
    "dotenv": "^17.0.1",
    "firebase": "^11.9.1",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "mssql": "^11.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.5",
    "@angular-eslint/builder": "20.0.0",
    "@angular-eslint/eslint-plugin": "20.0.0",
    "@angular-eslint/eslint-plugin-template": "20.0.0",
    "@angular-eslint/schematics": "20.0.0",
    "@angular-eslint/template-parser": "20.0.0",
    "@angular/cli": "^20.0.5",
    "@angular/compiler-cli": "^20.0.6",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "firebase-admin": "^13.4.0",
    "husky": "^9.1.7",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.5.2",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3"
  }
}
